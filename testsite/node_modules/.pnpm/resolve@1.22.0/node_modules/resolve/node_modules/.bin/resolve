#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules/.pnpm/resolve@1.22.0/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules/.pnpm/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/node_modules:/home/marting/dev/go/github.com/mazzegi/node_modules:/home/marting/dev/go/github.com/node_modules:/home/marting/dev/go/node_modules:/home/marting/dev/node_modules:/home/marting/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules/.pnpm/resolve@1.22.0/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules/.pnpm/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/testsite/node_modules:/home/marting/dev/go/github.com/mazzegi/bongo/node_modules:/home/marting/dev/go/github.com/mazzegi/node_modules:/home/marting/dev/go/github.com/node_modules:/home/marting/dev/go/node_modules:/home/marting/dev/node_modules:/home/marting/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/resolve" "$@"
else
  exec node  "$basedir/../../bin/resolve" "$@"
fi
